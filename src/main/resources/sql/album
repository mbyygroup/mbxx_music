CREATE TABLE `ALBUM` (
`ALBUM_ID` bigint(20) NOT NULL AUTO_INCREMENT COMMENT '表ID，主键',
`ALBUM_NAME` varchar(30) NOT NULL COMMENT '专辑名',
`SINGER_ID` bigint(20) NOT NULL COMMENT '歌手id',
PRIMARY KEY (`ALBUM_ID`),
UNIQUE KEY `ALBUM_NAME` (`ALBUM_NAME`)
)ENGINE=InnoDB AUTO_INCREMENT=1 DEFAULT CHARSET=utf8 COMMENT='专辑信息';